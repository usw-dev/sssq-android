<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android">

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android">

        <activity
            android:name=".Screen.screen_appver"
            android:exported="true" />
        <activity
            android:name=".Screen.ScreenTrhistoryActivity"
            android:exported="true" />
        <activity
            android:name=".Screen.screen_mymenu"
            android:exported="true" />
        <activity android:name=".Screen.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Screen.screen_main" />
        <activity android:name=".Screen.setting" />
    </application>

</manifest>